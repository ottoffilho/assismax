# =====================================================
# ASSISMAX - CONFIGURAÇÕES DE AMBIENTE (TEMPLATE)
# =====================================================
# Copie este arquivo para .env e preencha com suas credenciais reais
# NUNCA faça commit do arquivo .env com credenciais reais

# =====================================================
# SUPABASE - DATABASE & AUTH
# =====================================================
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Chaves para Edge Functions (backend only)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_JWT_SECRET=your_jwt_secret_here
SUPABASE_ACCESS_TOKEN=your_supabase_access_token_here

# =====================================================
# WHATSAPP BUSINESS API
# =====================================================
WHATSAPP_API_URL=https://graph.facebook.com/v18.0
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token_here
WHATSAPP_PHONE_NUMBER_ID=your_phone_number_id_here
WHATSAPP_BUSINESS_ACCOUNT_ID=your_business_account_id_here
WHATSAPP_WEBHOOK_VERIFY_TOKEN=your_webhook_verify_token_here

# =====================================================
# OPENAI / CLAUDE / DEEP SEEK- IA QUALIFICAÇÃO
# =====================================================
# API Keys (externos)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_MAX_TOKENS=1000

# Alternativa: Claude (Anthropic)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# =====================================================
# GOOGLE SHEETS API
# =====================================================
GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key_here
GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id_here
GOOGLE_SERVICE_ACCOUNT_EMAIL=your_service_account@project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_private_key_here\n-----END PRIVATE KEY-----"

# =====================================================
# EMAIL / SMTP CONFIGURATION
# =====================================================
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password_here
SMTP_FROM_NAME=AssisMax Atacarejo
SMTP_FROM_EMAIL=contato@assismax.com.br

# =====================================================
# CONFIGURAÇÕES GERAIS
# =====================================================
NODE_ENV=development
VITE_APP_NAME=AssisMax Atacarejo
VITE_APP_URL=http://localhost:8080
VITE_API_URL=http://localhost:8080/api

# Configurações de Rate Limiting
RATE_LIMIT_WHATSAPP=10
RATE_LIMIT_OPENAI=20
RATE_LIMIT_GOOGLE_SHEETS=100

# =====================================================
# ANALYTICS & MONITORING
# =====================================================
VITE_VERCEL_ANALYTICS_ID=your_vercel_analytics_id_here
SENTRY_DSN=your_sentry_dsn_here
SENTRY_AUTH_TOKEN=your_sentry_auth_token_here

# =====================================================
# CONFIGURAÇÕES ESPECÍFICAS ASSISMAX
# =====================================================
# Empresa padrão (slug da empresa no banco)
VITE_DEFAULT_EMPRESA_SLUG=assismax

# Configurações de captação de leads
VITE_LEAD_MODAL_DELAY=3000
VITE_LEAD_MODAL_SCROLL_TRIGGER=30

# Configurações regionais
VITE_DEFAULT_TIMEZONE=America/Sao_Paulo
VITE_DEFAULT_LOCALE=pt-BR
VITE_TARGET_REGION=Valparaíso de Goiás - GO

# =====================================================
# CONFIGURAÇÕES DE DESENVOLVIMENTO
# =====================================================
# Logs e Debug
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# Configurações de teste
VITEST_ENVIRONMENT=jsdom
TEST_TIMEOUT=10000

# =====================================================
# CONFIGURAÇÕES DE PRODUÇÃO
# =====================================================
# Descomente e configure para produção
# NODE_ENV=production
# VITE_APP_URL=https://assismax.com.br
# VITE_DEBUG_MODE=false
# VITE_LOG_LEVEL=error

# =====================================================
# WEBHOOKS & CALLBACKS
# =====================================================
WEBHOOK_SECRET=your_webhook_secret_here
CALLBACK_BASE_URL=https://your-domain.com/api/webhooks

# WhatsApp Webhook
WHATSAPP_WEBHOOK_URL=https://your-domain.com/api/webhooks/whatsapp

# N8N Webhook (Produção)
VITE_N8N_WEBHOOK_URL=https://assismax.app.n8n.cloud/webhook/webhook-test/assismax

# =====================================================
# BACKUP & STORAGE
# =====================================================
# AWS S3 (opcional para backup)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1
AWS_S3_BUCKET=assismax-backups

# =====================================================
# LGPD & COMPLIANCE
# =====================================================
LGPD_RETENTION_DAYS=365
AUDIT_LOG_ENABLED=true
CONSENT_TRACKING_ENABLED=true